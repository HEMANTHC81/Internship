from abc import ABC, abstractmethod

class Vehicle(ABC):
    @abstractmethod
    def start_engine(self):
        pass

class Car(Vehicle):
    def start_engine(self):
        print("Car engine started with key ignition.")

class Bike(Vehicle):
    def start_engine(self):
        print("Bike engine started with self-start button.")

class Bus(Vehicle):
    def start_engine(self):
        print("Bus engine started using air pressure system.")

c = Car()
b = Bike()
bs = Bus()

c.start_engine()
b.start_engine()
bs.start_engine()
